    "auth0-js": "^9.11.3",
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "highcharts": "^7.2.1",
    "highcharts-react-official": "^2.2.2",
    "lodash": "^4.17.15",
    "luxon": "^1.21.0",
    "mdi-material-ui": "^6.8.0",
    "memoize-one": "^5.1.1",
    "morgan": "^1.9.1",
    "mui-datatables": "^2.12.4",
    "prop-types": "^15.7.2",
    "qs": "^6.9.0",
    "query-string": "^6.8.3",
    "raf": "^3.4.1",
    "react": "^16.11.0",
    "react-apollo": "^3.1.3",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "react-scrollbars-custom": "^4.0.21",
    "react-swipeable-views": "^0.13.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
